include_rules

# This file is generated by the GNU configure script.
: |> ^o GEN %o^ cp -p @(AUTOTOOLS_STASH)/src/util/rose_paths.C . |> rose_paths.C

# Create a librose_util.so library, but also link the same objects into librose.so.
SOURCES = compilationFileDatabase.C LinearCongruentialGenerator.C processSupport.C \
          rose_getline.C rose_strtoull.C rose_paths.C

run $(support_compile_linklib) -o rose_util --objects=OBJECTS $(SOURCES)
: {OBJECTS} |> !for_librose |>

run $(public_header) compilationFileDatabase.h \
    LinearCongruentialGenerator.h Map.h timing.h ParallelSort.h processSupport.h ROSE_ABORT.h ROSE_ASSERT.h ROSE_UNUSED.h \
    rose_getline.h rose_isnan.h rose_paths.h rose_strtoull.h setup.h \
    rose_pragma_message.h
