include_rules

inputs = \
       SgAsmBinaryExpression.h \
       SgAsmExpression.h \
       SgAsmInstruction.h \
       SgAsmType.h

outputs = \
	generated/BasicTypes.h \
	generated/SgAsmBinaryExpression.h \
	generated/SgAsmBinaryExpression.C \
	generated/SgAsmExpression.h \
	generated/SgAsmExpression.C \
	generated/SgAsmInstruction.h \
	generated/SgAsmInstruction.C \
	generated/SgAsmType.h \
	generated/SgAsmType.C \
	generated/TreeNode.h

: ../../rosebud \
  |> LD_LIBRARY_PATH=$(ROSE)/src/Rosebud:$(ROSE)/src/util/Sawyer ../../rosebud --backend=rose --no-locations --rose-generated=generated $(inputs); cp ../Tree.h generated/TreeNode.h \
  |> $(outputs) <rosbud_example_1>

run $(tool_compile_linkexe) -I. test-1.C $(outputs)
run $(test) test-1
