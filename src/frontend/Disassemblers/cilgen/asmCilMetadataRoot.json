  {
    "Baseclass": "AsmCilNode",
    "Fields": [
      {
        "Doc": "Magic signature for physical metadata (always 0x424A5342)",
        "Init": "readExpected(read32bitValue, buf, index, MAGIC_SIGNATURE)",
        "Name": "Signature",
        "Type": "uint32_t"
      },
      {
        "Doc": "Major version number",
        "Init": "read16bitValue(buf, index)",
        "Name": "MajorVersion",
        "Type": "uint16_t"
      },
      {
        "Doc": "Minor version number",
        "Init": "read16bitValue(buf, index)",
        "Name": "MinorVersion",
        "Type": "uint16_t"
      },
      {
        "Doc": "Reserved space",
        "Init": "readExpected(read32bitValue, buf, index, 0)",
        "Name": "Reserved0",
        "Type": "uint32_t"
      },
      {
        "Doc": "Number of bytes allocated to hold version string (including null terminator)",
        "Init": "read32bitValue(buf, index)",
        "Name": "Length",
        "Type": "uint32_t"
      },
      {
        "Doc": "Version string",
        "Init": "readUtf8String(buf, index, get_Length())",
        "Name": "Version",
        "Type": "std::string"
      },
      {
        "Doc": "Padding to next 4-byte boundary",
        "Init": "readStringPadding(buf, index, p_Version.size(), get_Length())",
        "Name": "VersionPadding",
        "Type": "uint32_t"
      },
      {
        "Doc": "Reserved space",
        "Init": "readExpected(read16bitValue, buf, index, 0)",
        "Name": "Flags",
        "Type": "uint16_t"
      },
      {
        "Doc": "Reserved space",
        "Init": "read16bitValue(buf, index)",
        "Name": "NumberOfStreams",
        "Type": "uint16_t"
      }
    ],
    "Streams": [
      {
        "Doc": "Metadata streams",
        "Init": "parseStreams(this, buf, index, start_of_MetadataRoot, get_NumberOfStreams())",
        "Name": "Streams",
        "Type": "SgAsmCilDataStream"
      }
    ],
    "Name" : "AsmCilMetadataRoot"
  }
