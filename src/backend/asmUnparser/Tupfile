include_rules

SOURCES = BinaryEdgeArrows.C BinarySerialIo.C BinaryUnparserAarch32.C BinaryUnparserAarch64.C BinaryUnparserBase.C \
    BinaryUnparserM68k.C BinaryUnparserMips.C BinaryUnparserNull.C BinaryUnparserPowerpc.C BinaryUnparserX86.C AsmUnparser.C \
    AsmUnparser_compat.C AsmFunctionIndex.C unparseX86Asm.C unparsePowerpcAsm.C unparseM68kAsm.C unparseMipsAsm.C \
    unparseAarch64Asm.C

run $(librose_compile) $(SOURCES)

# Headers for the older API
run $(public_header) AsmUnparser.h AsmUnparser_compat.h AsmFunctionIndex.h

# Headers for the newer API
run $(public_header) BinaryEdgeArrows.h BinarySerialIo.h BinaryUnparser.h BinaryUnparserAarch32.h \
    BinaryUnparserAarch64.h BinaryUnparserBase.h BinaryUnparserM68k.h BinaryUnparserMips.h BinaryUnparserNull.h \
    BinaryUnparserPowerpc.h BinaryUnparserX86.h
