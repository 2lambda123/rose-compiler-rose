START

COMPOOL status'type;

TYPE status'1 STATUS
(
  V(LOW), % Low value, too low! %
  V(MED), % Medium value, just right. %
  V(HIGH) % High value, too high! %
);

TYPE status'2 STATUS
(
  %0% V(ZERO),
  %1% 1V(ONE),
  %2%  V(TWO),
  %3% 3V(THREE),
  %5% 5V(FIVE),
  %6%  V(SIX),
  %7%  V(SEVEN)
);

TERM
